#define OCCUPIED 1
#define AVAILABLE 0
#include <iostream>
using namespace std;

class Seat
{
private:
	bool status;
	Seat *next = NULL;
public:
	Seat()
	{
		this->status = AVAILABLE;
	}
};

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
